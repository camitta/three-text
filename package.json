{
  "name": "threejs-es6-webpack-boilerplate",
  "version": "1.0.0",
  "private": true,
  "description": "N/A",
  "main": "index.js",
  "scripts": {
    "clean": "rm -r -f dist/*",
    "lint": "eslint .",
    "lint-fix": "eslint --fix .",
    "build": " npm run clean && webpack --config webpack.prod.js",
    "build-dll": "webpack --config webpack.vendor.config.js",
    "start-dev": "webpack-dev-server --open --config webpack.dev.js", 
    "compress": "gltf-pipeline -i ./src/static/models/**/*.gltf dracoScene.gltf -d",
    "compress-all": "for path in ./src/static/models/*; do [ -d \"${path}\" ] || continue dirname=\"$(basename \"${path}\")\" done"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ethanny2/threejs-es6-webpack-barebones-boilerplate"
  },
  "keywords": [
    "Threejs"
  ],
  "author": "Ethan Soo Hon",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ethanny2/threejs-es6-webpack-barebones-boilerplate/issues"
  },
  "homepage": "https://github.com/ethanny2/PersonalSite#readme",
  "devDependencies": {
    "@babel/core": "^7.7.7",
    "@babel/preset-env": "^7.7.7",
    "autoprefixer": "^9.7.3",
    "babel-loader": "^8.0.6",
    "css-loader": "^3.4.0",
    "eslint": "^6.7.2",
    "eslint-config-google": "^0.14.0",
    "eslint-loader": "^3.0.3",
    "file-loader": "^5.0.2",
    "gltf-pipeline": "^2.1.4",
    "html-webpack-plugin": "^3.2.0",
    "image-webpack-loader": "^6.0.0",
    "mini-css-extract-plugin": "^0.8.2",
    "node-sass": "^4.13.0",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "postcss-loader": "^3.0.0",
    "sass-loader": "^8.0.0",
    "terser-webpack-plugin": "^2.3.1",
    "webpack": "^4.41.3",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.10.0",
    "webpack-merge": "^4.2.2"
  },
  "dependencies": {
    "@tweenjs/tween.js": "^18.5.0",
    "promise-polyfill": "8.1.3",
    "shader-particle-engine": "^1.0.6",
    "three": "^0.111.0"
  }
}
